    500: #607D8B,
    600: #546E7A,
    700: #455A64,
    800: #37474F,
    900: #263238
  ),
  black: (
    500: #000000
  ),
  white: (
    500: #ffffff
  )
);




// Fetch nested keys
@function fw-map-deep-get($map, $keys...) {
  @each $key in $keys {
    $map: map-get($map, $key);
  }
  @return $map;
}


// function for easy retreiving colors from MaterialUi Color Map
@function fw-color(
  $name: null,
  $shade: 500
){
  @if ( $name == white ) or ( $name == black ) { $shade: 500 }
  @return fw-map-deep-get($fw-material-colors, $name, $shade);
}

</splitSection>


<splitSection output="output/material_selectors.scss">



// selectors for background color
.fw-bg {
  // @each $KEY, $VALUE in $MAP
  @each $color, $colorShades in $fw-material-colors {
    // for each color
    &-#{$color} {
      @each $shade, $shadeValue in $colorShades {
        // for each shade
        &-#{$shade} {
          background-color: fw-color($color, $shade);
        }
        @if $shade == 500 {
          background-color: fw-color($color, $shade);
        }
      }
    }
  }
}


// selectors for text color
.fw {
  // @each $KEY, $VALUE in $MAP
  @each $color, $colorShades in $fw-material-colors {
    // for each color
    &-txt-#{$color} {
      @each $shade, $shadeValue in $colorShades {
        // for each shade
        &-#{$shade} {
          color: fw-color($color, $shade);
        }
        @if $shade == 500 {
          color: fw-color($color, $shade);
        }
      }
    }
  }
}

</splitSection>